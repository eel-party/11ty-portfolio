---
layout: base.njk
---
<p>this uses the "group" template</p>
{{ content | safe }}
<ul class="list flex justify-between ph0 mh0">
    {% for item in collections.item %}
    {% if item.data.ID == ID %}
    <li class="pv2 ph2">
        <a href="{{ item.url }}"><img src="/img/{{ item.data.image }}" alt="{{ description }}" class="nested-img img" /></a>
        <a href="{{ item.url }}">{{ item.data.title }}</a>
    </li>
    {% endif %}
    {% endfor %}
</ul>