---
layout: base.njk
---
<p>this uses the "page" template</p>
<p class="">
    {{ content | safe }}
</p>
<ul>
    {% for group in collections.group %}
    {% if group.data.eleventyNavigation.parent == eleventyNavigation.key %}
    <li>
        <div class="">
            <a href="{{ group.url | url }}"><img src="/img/{{ group.data.image }}" alt="{{ group.data.title }}" class="" /></a>
        </div>
        <div class="">
            <a href="{{ group.url | url }}">
                <h3 class="">{{ group.data.title }}</h3>
            </a>
            <p>{{ group.data.description }}</p>
        </div>
    </li>
    {% endif %}
    {% endfor %}
</ul>